{
    "dictionaries": [
        "bash",
        "python",
        "softwareTerms",
        "sphinx"
    ],
    "dictionaryDefinitions": [
        {
            "name": "sphinx",
            "path": "./dictionaries/sphinx.txt"
        }
    ],
    "ignorePaths": [
        "**/*.egg-info/**",
        "**/*.css",
        "**/*.js",
        "**/*.pyc",
        "**/*.ts",
        "**/.venv/**",
        "**/Pipfile.lock",
        "**/__pycache__/**",
        "**/build/**",
        "**/dist/**",
        "**/node_modules/**",
        "**/package-lock.json",
        "**/package.json",
        "**/poetry.lock",
        "**/requirements.txt",
        "**/tsconfig.json",
        ".vscode/dictionaries/*.txt",
        "typings/**"
    ],
    "language": "en",
    "languageSettings": [
        {
            "allowCompoundWords": true,
            "dictionaries": [
                "python"
            ],
            "languageId": "toml"
        }
    ],
    "maxNumberOfProblems": 100,
    "version": "0.2",
    "words": [
        "autofix",
        "boto",
        "direnv",
        "docstrings",
        "favicon",
        "fgrep",
        "frontmatter",
        "fstring",
        "furo",
        "howto",
        "kwarg",
        "labelmaker",
        "maint",
        "mdformat",
        "noqa",
        "overlining",
        "printf",
        "pydoc",
        "pyenv",
        "pygment",
        "pyright",
        "recommonmark",
        "repos",
        "subclassing",
        "ungrouped",
        "venv",
        "virtualenvs",
        "winget",
        "xargs",
        "yamlfmt"
    ]
